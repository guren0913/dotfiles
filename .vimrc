set encoding=euc-jp
set fileencodings=ucs-bom,utf-8,iso-2022-jp,sjis,cp932,euc-jp,cp20932
set fileformats=unix,dos,mac
set number
syntax on
:colorscheme koehler

set ruler
set laststatus=2

set list
set lcs=tab:>\ ,eol:<,trail:_,extends:\

set backspace=indent,eol,start

set tabstop=4
set shiftwidth=4
set expandtab
set foldmethod=marker

noremap zd zo
noremap zs zR
noremap zx zM
